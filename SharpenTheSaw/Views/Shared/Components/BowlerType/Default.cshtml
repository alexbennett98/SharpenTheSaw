@model IEnumerable<Teams>

<a asp-action="Index" asp-controller="Home" asp-route-bowlertypeid="">Home</a>
<br />  

@foreach(var x in Model)
{
    <a asp-action="Index" asp-controller="Home"
       asp-route-bowlertypeid="@x.TeamId"
       asp-route-bowlertype="@x.TeamName">
        @x.TeamName
    </a>
    <br />
}